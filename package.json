{
  "name": "ur-cache-cleaner",
  "displayName": "UR Cache Cleaner",
  "description": "Clear cache from CMS like Drupal.",
  "publisher": "Clear cache from CMS like Drupal.",
  "author": {
    "name": "Vinay Gawade"
  },
  "icon": "assets/images/logo.png",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "preview": true,
  "activationEvents": [
    "workspaceContains:**/composer.json"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "icons": {
      "clear-cache": {
        "description": "Clear cache icon",
        "default": {
          "fontPath": "./assets/icons/fonts/ur-font.woff",
          "fontCharacter": "\\43"
        }
      },
      "loading-icon": {
        "description": "Loading icon",
        "default": {
          "fontPath": "./assets/icons/fonts/ur-font.woff",
          "fontCharacter": "\\4f"
        }
      },
      "info-icon": {
        "description": "Info icon",
        "default": {
          "fontPath": "./assets/icons/fonts/ur-font.woff",
          "fontCharacter": "\\49"
        }
      },
      "apple-icon": {
        "description": "Apple system icon",
        "default": {
          "fontPath": "./assets/icons/fonts/ur-font.woff",
          "fontCharacter": "\\41"
        }
      },
      "unix-icon": {
        "description": "Unix system icon",
        "default": {
          "fontPath": "./assets/icons/fonts/ur-font.woff",
          "fontCharacter": "\\75"
        }
      },
      "windows-icon": {
        "description": "Wiindows system icon",
        "default": {
          "fontPath": "./assets/icons/fonts/ur-font.woff",
          "fontCharacter": "\\77"
        }
      },
      "code-icon": {
        "description": "Code icon",
        "default": {
          "fontPath": "./assets/icons/fonts/ur-font.woff",
          "fontCharacter": "\\48"
        }
      }
    },
    "commands": [
      {
        "command": "ur-cache-cleaner.drushClearCache",
        "title": "Drush Clear Cache"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.72.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "command-exists": "^1.2.9"
  },
  "license": "",
  "bugs": {
    "url": "https://github.com/vinugawade/ur-cache-cleaner/issues",
    "email": "vinulike11@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vinugawade/ur-cache-cleaner.git"
  },
  "homepage": "https://github.com/vinugawade/ur-cache-cleaner/blob/master/README.md"
}
